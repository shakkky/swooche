{
  "name": "swooche-mobile",
  "version": "1.0.0",
  "main": "Main.tsx",
  "scripts": {
    "start": "expo start --dev-client --lan",
    "dev:android": "expo start --android",
    "dev:ios": "expo start --ios",
    "web": "expo start --web",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "clean": "rimraf .expo android coverage ios",
    "check-types": "tsc --noEmit --pretty",
    "build:apk": "eas build --platform android --profile preview",
    "build:apk-dev": "eas build --platform android --profile development",
    "build:apk-prod": "eas build --platform android --profile production",
    "build:ios": "eas build --platform ios --profile preview",
    "build:ios-dev": "eas build --platform ios --profile development",
    "build:ios-prod": "eas build --platform ios --profile production"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@trpc/client": "^11.0.0",
    "@trpc/server": "^11.0.0",
    "@twilio/voice-sdk": "^2.14.0",
    "expo": "~53.0.0",
    "expo-av": "^15.1.7",
    "expo-constants": "~17.1.7",
    "expo-contacts": "^14.2.5",
    "expo-dev-client": "~5.2.4",
    "expo-device": "^7.1.4",
    "expo-font": "^13.3.2",
    "expo-linking": "~7.1.7",
    "expo-notifications": "^0.31.4",
    "expo-router": "~5.1.3",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "nativewind": "^2.0.11",
    "react": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-web": "^0.20.0",
    "react-native-webrtc": "^124.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@expo/metro-config": "^0.20.17",
    "@types/react": "19.0.8",
    "@types/react-native": "^0.72.2",
    "babel-plugin-module-resolver": "^5.0.2",
    "eas-cli": "^3.17.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.1",
    "tailwindcss": "3.3.2",
    "typescript": "^5.1.6"
  }
